package BookInfor;

public class JoinVO {
	private String id;
	private String password;
	
	
	public JoinVO() {
		
	}
	
	public JoinVO(String id, String password) {
		this.id= id;
		this.password = password;
		}
	
	public void joinprint() {
		System.out.printf("%10s %10s\n",id,password);
	}
	public static void jointitle() {
		System.out.println("==================================================================================");
		System.out.println("\t\t회원목록");
		System.out.println("\t아이디\t비밀번호");
		System.out.println("==================================================================================");
	}
	
	public String getId() {
		return id;
	}
	
	public void setId(String id) {
		this.id = id;
	}
	
	public String getPassword() {
		return password;
	}
	
	public void setPassword(String password) {
		this.password = password;
	}
	


}


