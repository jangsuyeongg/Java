package BookInfor;

import java.util.HashMap;

public class BookDataSet {
		public static HashMap<String, BookVO> bookList = new HashMap<String, BookVO>();
		
		public BookDataSet() {
			
		}
		
		public static void basicBookSet() {
			bookList.put("그렇게 그렇게", new BookVO(1001, "그렇게 그렇게" ,"양지연","주니어김영사","대여가능"," "));
			bookList.put("달팽이", new BookVO(1002, "달팽이" ,"김민우","웅진주니어","대여중","2021.05.10"));

			
			
			
		}
		
		
	

}
