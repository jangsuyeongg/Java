package BookInfor;

public class JoinVO {
	private String id;
	private String password;
	
	
	public JoinVO() {
		
	}
	
	public void joinprint() {
		System.out.printf("%10d %10d\n",id,password);
	}
	public void jointitle() {
		System.out.printf("%10d %10d\n","id","password");
	}
	
public JoinVO(String id, String password) {
	this.id= id;
	this.password = password;
	}

public String getId() {
	return id;
}

public void setId(String id) {
	this.id = id;
}

public String getPassword() {
	return password;
}

public void setPassword(String password) {
	this.password = password;
}




}


