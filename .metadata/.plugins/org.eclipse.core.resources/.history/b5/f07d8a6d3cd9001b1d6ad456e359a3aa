import javax.swing.JFrame;
//								default : BorderLayout
public class JFrameTest extends JFrame {

	public JFrameTest() {
		setSize(500,500);
		setVisible(true);
		//프로그램이 종료되면 자원해제하거나, 프로그램을 종료할수없도록 설정.......
		setDefaultCloseOperation(DISPOSE_ON_CLOSE);
	}

	

	public static void main(String[] args) {
		new JFrameTest();

	}

}
