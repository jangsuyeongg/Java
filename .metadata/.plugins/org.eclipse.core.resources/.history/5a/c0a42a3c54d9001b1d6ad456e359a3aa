import java.awt.Frame;
import javax.swing.JFrame;

public class DigitalClock extends JFrame {
		Frame frm = new Frame("시계");
	public DigitalClock() {
		
		
		
		frm.setSize(1000,1000);
		setVisible(true);
		
		setTitle("시계");
	}

	public static void main(String[] args) {
		new DigitalClock();

	}

}
